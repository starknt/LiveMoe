{
  "name": "livemoe",
  "description": "一款基于 Electron 的免费开源桌面壁纸播放器",
  "keywords": [
    "electron",
    "react",
    "typescript",
    "ts",
    "webpack",
    "hot",
    "reload",
    "livemoe",
    "wallpaper",
    "wallpaper-player",
    "wallpaper engine"
  ],
  "license": "MIT",
  "author": {
    "name": "Seven",
    "email": "1431880400@qq.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SEVEN-1-bit/LiveMoe.git"
  },
  "scripts": {
    "build": "npm run clean && cross-env NODE_ENV=production TS_NODE_TRANSPILE_ONLY=true concurrently \"npm run build:main\" \"npm run build:renderer\" \"npm run build:preload\" \"npm run build:plugin\"",
    "build:main": "cross-env NODE_ENV=production TS_NODE_TRANSPILE_ONLY=true webpack --config ./.erb/configs/webpack.config.main.prod.ts",
    "build:renderer": "cross-env NODE_ENV=production TS_NODE_TRANSPILE_ONLY=true webpack --config ./.erb/configs/webpack.config.renderer.prod.ts",
    "build:preload": "cross-env NODE_ENV=production TS_NODE_TRANSPILE_ONLY=true webpack --config ./.erb/configs/webpack.config.preload.prod.ts",
    "build:plugin": "cross-env NODE_ENV=production TS_NODE_TRANSPILE_ONLY=true concurrently \"webpack --config ./.erb/configs/webpack.config.plugin.backend.ts\" \"webpack --config ./.erb/configs/webpack.config.plugin.frontend.ts\"",
    "rebuild": "electron-rebuild --parallel --types prod,dev,optional --module-dir src",
    "lint": "cross-env NODE_ENV=development eslint . --cache --ext .js,.jsx,.ts,.tsx",
    "package": "npm run clean && npm run build && electron-builder build",
    "publish": "npm run clean && npm run build && electron-builder build --publish always",
    "postinstall": "ts-node .erb/scripts/check-native-dep.js && electron-builder install-app-deps && cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true webpack --config ./.erb/configs/webpack.config.renderer.dev.dll.ts",
    "dev:main": "cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true electron -r ts-node/register -r tsconfig-paths/register ./src/electron-main/main.ts",
    "dev:renderer": "cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true webpack serve --config ./.erb/configs/webpack.config.renderer.dev.ts",
    "dev:preload": "cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true webpack --config ./.erb/configs/webpack.config.preload.dev.ts",
    "dev:plugin": "cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true concurrently \"webpack --config ./.erb/configs/webpack.config.plugin.backend.ts\" \"webpack --config ./.erb/configs/webpack.config.plugin.frontend.ts\"",
    "clean": "ts-node ./.erb/scripts/clean.js dist plugin",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "prepare": "husky install"
  },
  "dependencies": {
    "@fontsource/roboto": "^4.5.3",
    "@mui/icons-material": "^5.5.1",
    "@mui/material": "^5.5.2",
    "@mui/styled-engine-sc": "^5.5.2",
    "@reduxjs/toolkit": "^1.8.0",
    "animejs": "^3.2.1",
    "chokidar": "^3.5.2",
    "classnames": "^2.3.1",
    "electron-localshortcut": "^3.2.1",
    "electron-log": "^4.4.1",
    "electron-updater": "^4.3.9",
    "fs-extra": "^10.1.0",
    "i18next": "^21.5.4",
    "minimist": "^1.2.5",
    "pretty-bytes": "^6.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-flip-toolkit": "^7.0.13",
    "react-i18next": "^11.14.3",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.2.1",
    "styled-components": "^5.3.3",
    "yauzl": "^2.10.0",
    "yazl": "^2.5.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.20.7",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.5",
    "@teamsupercell/typings-for-css-modules-loader": "^2.5.1",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@types/animejs": "^3.1.4",
    "@types/copy-webpack-plugin": "^8.0.1",
    "@types/enzyme": "^3.10.10",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/fs-extra": "^9.0.13",
    "@types/minimist": "^1.2.2",
    "@types/node": "16.11.6",
    "@types/pouchdb": "^6.4.0",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@types/react-router": "^5.1.17",
    "@types/react-router-dom": "^5.3.2",
    "@types/react-test-renderer": "^17.0.1",
    "@types/styled-components": "^5.1.22",
    "@types/terser-webpack-plugin": "^5.0.4",
